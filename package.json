{
  "name": "MyVueProject",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "autoprefix": "npx postcss scss/main.scss --use autoprefixer -d dist/",
    "build-css-dev": "sass scss/main.scss:dist/main.css --update --style=expanded --stop-on-error",
    "build-css": "sass scss/main.scss:dist/main.css --style=compressed --stop-on-error",
    "watch-css-dev": "sass scss/main.scss:dist/main.css --update --style=expanded --stop-on-error --watch",

    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "browsersync": "browser-sync start --server --files \"dist/*\"",
    "resolveModulesWithWebpack": "webpack",
    "purifycss": "purifycss dist/main.css dist/main.purified.css -i -r ",
    "all-commands": "npm-run-all build-css build-ts --parallel watch-css watch-ts browsersync resolveModulesWithWebpack",
    "": "watch 'npm run all-commands' src/*"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "common-js": "^0.3.8"
  },
  "devDependencies": {
    "amd": "0.0.0",
    "autoprefixer": "^9.7.0",
    "bootstrap": "^4.3.1",
    "browser-sync": "^2.26.7",
    "dart-sass": "^1.23.7",
    "jquery": "^3.4.1",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "popper": "^1.0.1",
    "postcss-cli": "^6.1.3",
    "purify-css": "^1.2.5",
    "sass": "^1.23.7",
    "tsc-watch": "^4.0.0",
    "typescript": "^3.6.4",
    "vue": "^2.6.10",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9"
  }
}
