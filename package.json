{
  "name": "MyVueProject",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "autoprefix": "npx postcss *.css --use autoprefixer -d build/",
    "build-css": "node-sass --include-path scss scss/main.scss   dist/main.css \"npm run purifycss\"",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "browsersync": "browser-sync start --server --files \"dist/*\"",
    "resolveModulesWithWebpack": "webpack",
    "purifycss": "purifycss dist/main.css dist/main.purified.css -i -r ",
    "start": "npm-run-all build-css build-ts --parallel watch-css watch-ts browsersync resolveModulesWithWebpack"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "common-js": "^0.3.8"
  },
  "devDependencies": {
    "autoprefixer": "^9.7.0",
    "bootstrap": "^4.3.1",
    "browser-sync": "^2.26.7",
    "jquery": "^3.4.1",
    "node-sass": "^4.13.0",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "popper": "^1.0.1",
    "postcss-cli": "^6.1.3",
    "purify-css": "^1.2.5",
    "tsc-watch": "^4.0.0",
    "typescript": "^3.6.4",
    "vue": "^2.6.10",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9"
  }
}
