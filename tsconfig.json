{
  "compilerOptions": {
    "target": "ES5",
    "module": "amd",
    "removeComments": true,
    "preserveConstEnums": true,
    "outFile": "dist/main.js",
    "sourceRoot": "ts",
    "outDir": "dist/",
    "rootDir": "./",
    "sourceMap": true,
    "types" : [ "node" ]
  },
  "include": [
    "**/*"
  ],
  "exclude": [
    "node_modules/*",
    "**/*.spec.ts/*",
    "dist/*"
  ]
}
